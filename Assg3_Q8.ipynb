   ],
   "source": [
    "n=int(input(\"Enter the number:\"))\n",
    "temp = n\n",
    "rev = 0\n",
    "while n>0:\n",
    "      num=n%10\n",
    "      rev=rev*10+num\n",
    "      n=n//10\n",
    "if temp==rev:\n",
    "      print(\"The number is a Palindrome.\")\n",
    "else:\n",
    "      print(\"The number is not a Palindrome.\")"
   ]
